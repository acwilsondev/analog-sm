generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Member {
  id        String   @id @default(cuid())
  email     String   @unique
  role      String
  createdAt DateTime @default(now())
}

model BootstrapState {
  id              Int      @id @default(1)
  bootstrapUsedAt DateTime?
}

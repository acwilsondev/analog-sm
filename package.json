{
  "name": "analog-sm",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/app/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/app/server.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "node --test",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "db:migrate": "bash scripts/exec-in-app.sh prisma migrate deploy",
    "db:migrate:dev": "bash scripts/exec-in-app.sh prisma migrate dev",
    "create-admin": "bash scripts/exec-in-app.sh node dist/scripts/create-admin.js",
    "backup": "bash scripts/backup.sh",
    "restore": "bash scripts/restore.sh",
    "backup:validate": "bash scripts/backup-restore-validate.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pg": "^8.13.1",
    "zod": "^3.24.1",
    "prisma": "^6.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.6",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "@types/pg": "^8.11.10"
  }
}
